<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/background_color"
    tools:ignore="HardcodedText,Autofill,TextFields">

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/bike1ToProfileSelectButtonId"
        android:layout_width="80dp"
        android:layout_height="30dp"
        android:background="@drawable/button_background"
        android:fontFamily="@font/airstrike"
        android:foreground="@drawable/button_foreground"
        android:gravity="center"
        android:paddingHorizontal="4sp"
        android:text="back"
        android:textColor="@color/white"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.049"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.981" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/bike1ToHomeButtonId"
        android:layout_width="80dp"
        android:layout_height="30dp"
        android:background="@drawable/button_background"
        android:fontFamily="@font/airstrike"
        android:foreground="@drawable/button_foreground"
        android:gravity="center"
        android:paddingHorizontal="4sp"
        android:text="home"
        android:textColor="@color/white"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.981" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/bike1SubmitButtonId"
        android:layout_width="180dp"
        android:layout_height="30dp"
        android:layout_marginBottom="40dp"
        android:background="@drawable/button_background"
        android:fontFamily="@font/airstrike"
        android:foreground="@drawable/button_foreground"
        android:gravity="center"
        android:paddingHorizontal="4sp"
        android:text="submit"
        android:textColor="@color/white"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/bike1NotesButtonId"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/bike1NotesButtonId"
        android:layout_width="100dp"
        android:layout_height="30dp"
        android:background="@drawable/button_background"
        android:fontFamily="@font/airstrike"
        android:foreground="@drawable/button_foreground"
        android:gravity="center"
        android:layout_marginBottom="20dp"
        android:paddingHorizontal="4sp"
        android:text="notes"
        android:textColor="@color/white"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/bike1ToHomeButtonId"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/bike1DatePickerButtonId"
        style="?android:spinnerStyle"
        android:layout_width="140dp"
        android:layout_height="30dp"
        android:background="@drawable/button_background"
        android:fontFamily="@font/airstrike"
        android:foreground="@drawable/button_foreground"
        android:gravity="center"
        android:paddingHorizontal="8sp"
        android:paddingVertical="4dp"
        android:layout_marginBottom="20dp"
        android:text="JAN 01 2020"
        android:textColor="@color/white"
        android:textSize="14sp"
        app:layout_constraintBottom_toTopOf="@+id/bike1SubmitButtonId"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <EditText
        android:id="@+id/bike1KmSinceMaintenanceEditId"
        android:layout_width="100dp"
        android:layout_height="30dp"
        android:layout_marginBottom="20dp"
        android:background="@drawable/edit_background"
        android:fontFamily="@font/airstrike"
        android:foreground="@drawable/text_foreground"
        android:gravity="center"
        android:hint="km"
        android:paddingHorizontal="8sp"
        android:textColor="@color/white"
        android:textSize="14sp"
        app:layout_constraintBottom_toTopOf="@+id/bike1LastMaintenanceTextId"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <EditText
        android:id="@+id/bike1TotalKmEditId"
        android:layout_width="100dp"
        android:layout_height="30dp"
        android:layout_marginBottom="20dp"
        android:background="@drawable/edit_background"
        android:fontFamily="@font/airstrike"
        android:foreground="@drawable/text_foreground"
        android:gravity="center"
        android:hint="km"
        android:paddingHorizontal="8sp"
        android:textColor="@color/white"
        android:textSize="14sp"
        app:layout_constraintBottom_toTopOf="@+id/bike1KmThisMonthTextId"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1" />

    <EditText
        android:id="@+id/bike1KmThisMonthEditId"
        android:layout_width="100dp"
        android:layout_height="30dp"
        android:layout_marginBottom="20dp"
        android:background="@drawable/edit_background"
        android:fontFamily="@font/airstrike"
        android:foreground="@drawable/text_foreground"
        android:gravity="center"
        android:hint="km"
        android:paddingHorizontal="4sp"
        android:paddingVertical="8dp"
        android:textColor="@color/white"
        android:textSize="14sp"
        app:layout_constraintBottom_toTopOf="@+id/bike1KmSinceMaintenanceTextId"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1" />

    <TextView
        android:id="@+id/bike1KmSinceMaintenanceTextId"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="10dp"
        android:fontFamily="@font/airstrike"
        android:gravity="center"
        android:paddingStart="4dp"
        android:paddingEnd="8dp"
        android:text="bike_1_km_since_maintenance_text_id"
        android:textColor="@color/white"
        android:textSize="14sp"
        app:layout_constraintBottom_toTopOf="@+id/bike1KmSinceMaintenanceEditId"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/bike1KmThisMonthTextId"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="10dp"
        android:fontFamily="@font/airstrike"
        android:gravity="center"
        android:paddingStart="4dp"
        android:paddingEnd="8dp"
        android:text="bike_1_km_this_month_text_id"
        android:textColor="@color/white"
        android:textSize="14sp"
        app:layout_constraintBottom_toTopOf="@+id/bike1KmThisMonthEditId"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/bike1TotalKmTextId"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="10dp"
        android:fontFamily="@font/airstrike"
        android:gravity="center"
        android:paddingStart="4dp"
        android:paddingEnd="8dp"
        android:text="bike_1_total_km_text_id"
        android:textColor="@color/white"
        android:textSize="14sp"
        app:layout_constraintBottom_toTopOf="@+id/bike1TotalKmEditId"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/bike1LastMaintenanceTextId"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="10dp"
        android:fontFamily="@font/airstrike"
        android:gravity="center"
        android:paddingStart="4dp"
        android:paddingEnd="8dp"
        android:text="bike_1_last_maintenance_text_id"
        android:textColor="@color/white"
        android:textSize="14sp"
        app:layout_constraintBottom_toTopOf="@+id/bike1DatePickerButtonId"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/bike1TextId"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="100dp"
        android:fontFamily="@font/airstrike"
        android:gravity="center"
        android:paddingStart="4dp"
        android:paddingEnd="8dp"
        android:text="bike_1_text_id"
        android:textColor="@color/white"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0" />

</androidx.constraintlayout.widget.ConstraintLayout>