<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/background_color"
    tools:ignore="HardcodedText,Autofill,TextFields">

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/bike1BackButtonID"
        android:layout_width="80dp"
        android:layout_height="30dp"
        android:layout_marginStart="8dp"
        android:layout_marginBottom="12dp"
        android:background="@drawable/button_background"
        android:fontFamily="@font/airstrike"
        android:foreground="@drawable/button_foreground"
        android:gravity="center"
        android:paddingHorizontal="4sp"
        android:text="back"
        android:textColor="@color/white"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/bike1ToHomeButtonID"
        android:layout_width="80dp"
        android:layout_height="30dp"
        android:layout_marginBottom="12dp"
        android:background="@drawable/button_background"
        android:fontFamily="@font/airstrike"
        android:foreground="@drawable/button_foreground"
        android:gravity="center"
        android:paddingHorizontal="4sp"
        android:text="home"
        android:textColor="@color/white"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/bike1SubmitButtonID"
        android:layout_width="180dp"
        android:layout_height="30dp"
        android:layout_marginBottom="80dp"
        android:background="@drawable/button_background"
        android:fontFamily="@font/airstrike"
        android:foreground="@drawable/button_foreground"
        android:gravity="center"
        android:paddingHorizontal="4sp"
        android:text="submit"
        android:textColor="@color/white"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/bike1NotesButtonID"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/bike1NotesButtonID"
        android:layout_width="100dp"
        android:layout_height="30dp"
        android:background="@drawable/button_background"
        android:fontFamily="@font/airstrike"
        android:foreground="@drawable/button_foreground"
        android:gravity="center"
        android:layout_marginBottom="20dp"
        android:paddingHorizontal="4sp"
        android:text="notes"
        android:textColor="@color/white"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/bike1ToHomeButtonID"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/bike1DatePickerButtonID"
        style="?android:spinnerStyle"
        android:layout_width="120dp"
        android:layout_height="30dp"
        android:background="@drawable/button_background"
        android:fontFamily="@font/airstrike"
        android:foreground="@drawable/text_foreground"
        android:gravity="center"
        android:paddingHorizontal="8sp"
        android:paddingVertical="4dp"
        android:layout_marginBottom="20dp"
        android:text="JAN 01 2020"
        android:textColor="@color/background_color"
        android:textSize="14sp"
        app:layout_constraintBottom_toTopOf="@+id/bike1SubmitButtonID"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <EditText
        android:id="@+id/bike1KmSinceMaintenanceEditID"
        android:layout_width="120dp"
        android:layout_height="30dp"
        android:layout_marginBottom="20dp"
        android:background="@drawable/edit_background"
        android:fontFamily="@font/airstrike"
        android:foreground="@drawable/text_foreground"
        android:gravity="center"
        android:hint="km"
        android:paddingHorizontal="8sp"
        android:textColor="@color/white"
        android:textSize="14sp"
        app:layout_constraintBottom_toTopOf="@+id/bike1LastMaintenanceTextID"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <EditText
        android:id="@+id/bike1TotalKmEditID"
        android:layout_width="120dp"
        android:layout_height="30dp"
        android:layout_marginBottom="20dp"
        android:background="@drawable/edit_background"
        android:fontFamily="@font/airstrike"
        android:foreground="@drawable/text_foreground"
        android:gravity="center"
        android:hint="km"
        android:paddingHorizontal="8sp"
        android:textColor="@color/white"
        android:textSize="14sp"
        app:layout_constraintBottom_toTopOf="@+id/bike1KmThisMonthTextID"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1" />

    <EditText
        android:id="@+id/bike1KmThisMonthEditID"
        android:layout_width="120dp"
        android:layout_height="30dp"
        android:layout_marginBottom="20dp"
        android:background="@drawable/edit_background"
        android:fontFamily="@font/airstrike"
        android:foreground="@drawable/text_foreground"
        android:gravity="center"
        android:hint="km"
        android:paddingHorizontal="4sp"
        android:paddingVertical="8dp"
        android:textColor="@color/white"
        android:textSize="14sp"
        app:layout_constraintBottom_toTopOf="@+id/bike1KmSinceMaintenanceTextID"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1" />

    <TextView
        android:id="@+id/bike1KmSinceMaintenanceTextID"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="10dp"
        android:fontFamily="@font/airstrike"
        android:gravity="center"
        android:paddingStart="4dp"
        android:paddingEnd="8dp"
        android:text="bike_1_km_since_maintenance_text_id"
        android:textColor="@color/white"
        android:textSize="14sp"
        app:layout_constraintBottom_toTopOf="@+id/bike1KmSinceMaintenanceEditID"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/bike1KmThisMonthTextID"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="10dp"
        android:fontFamily="@font/airstrike"
        android:gravity="center"
        android:paddingStart="4dp"
        android:paddingEnd="8dp"
        android:text="bike_1_km_this_month_text_id"
        android:textColor="@color/white"
        android:textSize="14sp"
        app:layout_constraintBottom_toTopOf="@+id/bike1KmThisMonthEditID"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/bike1TotalKmTextID"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="10dp"
        android:fontFamily="@font/airstrike"
        android:gravity="center"
        android:paddingStart="4dp"
        android:paddingEnd="8dp"
        android:text="bike_1_total_km_text_id"
        android:textColor="@color/white"
        android:textSize="14sp"
        app:layout_constraintBottom_toTopOf="@+id/bike1TotalKmEditID"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/bike1LastMaintenanceTextID"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="10dp"
        android:fontFamily="@font/airstrike"
        android:gravity="center"
        android:paddingStart="4dp"
        android:paddingEnd="8dp"
        android:text="bike_1_last_maintenance_text_id"
        android:textColor="@color/white"
        android:textSize="14sp"
        app:layout_constraintBottom_toTopOf="@+id/bike1DatePickerButtonID"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/bike1TextID"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="100dp"
        android:fontFamily="@font/airstrike"
        android:gravity="center"
        android:paddingStart="4dp"
        android:paddingEnd="8dp"
        android:text="bike_1_text_id"
        android:textColor="@color/white"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0" />

</androidx.constraintlayout.widget.ConstraintLayout>